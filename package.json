{
  "name": "mdkk-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/mdkk-ui.umd.js",
  "module": "./dist/mdkk-ui.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/mdkk-ui.es.js",
      "require": "./dist/mdkk-ui.umd.js"
    },
    "./dist/mdkk-ui.css": "./dist/mdkk-ui.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "format": "biome format",
    "format:fix": "biome format --write",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write --unsafe .",
    "check": "biome check",
    "check:fix": "biome lint --write --unsafe . && biome format --write .",
    "typecheck": "tsc --noEmit",
    "generate:figma-types": "openapi-typescript ./node_modules/@figma/rest-api-spec/openapi/openapi.yaml -o ./src/ui/libs/open-api/figma-api.d.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "cva": "1.0.0-beta.4",
    "react-aria-components": "^1.14.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@chromatic-com/storybook": "^4.1.3",
    "@eslint/js": "^9.39.1",
    "@storybook/addon-a11y": "^10.1.11",
    "@storybook/addon-docs": "^10.1.11",
    "@storybook/addon-onboarding": "^10.1.11",
    "@storybook/addon-vitest": "^10.1.11",
    "@storybook/react-vite": "^10.1.11",
    "@storybook/test": "^8.6.15",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/browser-playwright": "^4.0.16",
    "@vitest/coverage-v8": "^4.0.16",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-storybook": "^10.1.11",
    "globals": "^16.5.0",
    "happy-dom": "^20.0.11",
    "jsdom": "^27.0.1",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "storybook": "^10.1.11",
    "tailwindcss": "^3.4.17",
    "tailwindcss-react-aria-components": "^2.0.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.16"
  }
}
